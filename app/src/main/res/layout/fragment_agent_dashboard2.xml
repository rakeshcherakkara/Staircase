<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="25dp">

    <View
        android:id="@+id/view4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="411dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/flamingo_dashboard_header" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.33" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.66" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="271dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="395dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="470dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="545dp" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_weight="3">

            <Button
                android:id="@+id/SaleButton"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="Sale"
                android:textColor="@color/colorWhite"
                android:theme="@style/Widget.AppCompat.Button.Colored"
                tools:layout_editor_absoluteY="319dp" />

            <Button
                android:id="@+id/SaleReportButtom"
                style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="Sale Report"
                android:textColor="@color/colorWhite"
                tools:layout_editor_absoluteX="122dp"
                tools:layout_editor_absoluteY="319dp" />

            <Button
                android:id="@+id/WinningReportButton"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="Winning Report"
                android:textColor="@color/colorWhite"
                tools:layout_editor_absoluteX="253dp"
                tools:layout_editor_absoluteY="319dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_weight="3">

            <Button
                android:id="@+id/ResultReportButton"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="Result Report"
                android:textColor="@color/colorWhite"
                tools:layout_editor_absoluteY="399dp" />

            <Button
                android:id="@+id/AmountWiseReportButton"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="Amountwise Report"
                android:textColor="@color/colorWhite"
                tools:layout_editor_absoluteY="399dp" />

            <Button
                android:id="@+id/AdminPayReportButton"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="Payment Report (Admin)"
                android:textColor="@color/colorWhite"
                tools:layout_editor_absoluteY="399dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_weight="3">

            <Button
                android:id="@+id/SunagentPayReportButton"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="Payment Report (SUB-Agent)"
                android:textColor="@color/colorWhite"
                tools:layout_editor_absoluteX="127dp"
                tools:layout_editor_absoluteY="479dp" />

            <Button
                android:id="@+id/SubagentPayButton"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="SUb Agent Payment"
                android:textColor="@color/colorWhite"
                tools:layout_editor_absoluteY="479dp" />

            <Button
                android:id="@+id/CreateSubAgentButton"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@drawable/button_drawable"
                android:text="Create Sub Agent"
                android:textColor="@color/colorWhite"
                tools:layout_editor_absoluteX="253dp"
                tools:layout_editor_absoluteY="479dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </TableLayout>

</android.support.constraint.ConstraintLayout>
